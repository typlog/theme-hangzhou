<div id="navbar" class="z-30 fixed lg:sticky top-0 w-full">
  <div id="navbar-backdrop" class="absolute w-full h-full backdrop-blur border-b"></div>
  <div class="mx-auto max-w-content relative">
    {% include "./site-head.j2" %}

    <div class="flex items-center justify-between h-12 py-4 px-5 lg:hidden border-t">
      <div class="flex text-sm leading-6 whitespace-nowrap min-w-0 space-x-3">
        <button class="js-menu inline-flex items-center gap-2 p-1" aria-label="{{ translate('menu') }}" aria-controls="doc-sidebar">
          <svg xmlns="http://www.w3.org/2000/svg" width="1em" height="1em" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2">
            <path d="M4 12h16M4 18h16M4 6h16"></path>
          </svg>
          <span class="inline md:hidden">{{ translate('menu') }}</span>
        </button>
        <div class="hidden md:flex items-center gap-1 text-sm text-gray-11">
          <span>{{ column.title }}</span>
          <svg class="text-gray-9" xmlns="http://www.w3.org/2000/svg" width="14" height="14" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
            <path d="m9 18 6-6-6-6"/>
          </svg>
        </div>
        <div class="hidden md:flex items-center text-sm font-medium">
          <span>{{ note.title }}</span>
        </div>
      </div>
      <div class="flex items-center">
        <button class="js-menu flex items-center gap-1 text-sm" aria-expanded="false" aria-controls="doc-outline">
          <svg xmlns="http://www.w3.org/2000/svg" width="1em" height="1em" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
            <path d="M8 6h10"/>
            <path d="M6 12h9"/>
            <path d="M11 18h7"/>
          </svg>
          <span>{{ translate('onthispage') }}</span>
        </button>
      </div>
    </div>

    <div class="hidden lg:flex justify-between px-4 lg:px-12 h-12">
      <div class="w-full h-full flex justify-between border-t">
        <div class="nav-tabs h-full flex text-sm space-x-6">
          {% for item in columns %}
            <a class="nav-tabs-item{% if column.id == item.id %} active{% endif %}"
              href="{% if item.metadata.url %}{{ item.metadata.url }}{% else %}{{ item.url }}{% endif %}"
            >
              {% if item.metadata.icon %}
                <iconify-icon icon="{{ item.metadata.icon }}"></iconify-icon>
              {% endif %}
              <span>{{ item.title }}</span>
            </a>
          {% endfor %}
        </div>
        <div class="flex items-center xl:hidden">
          <button class="js-menu flex items-center gap-1 text-sm" aria-expanded="false" aria-controls="doc-outline">
            <svg xmlns="http://www.w3.org/2000/svg" width="1em" height="1em" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
              <path d="M8 6h10"/>
              <path d="M6 12h9"/>
              <path d="M11 18h7"/>
            </svg>
            <span>{{ translate('onthispage') }}</span>
          </button>
        </div>
      </div>
    </div>
  </div>
</div>
<div id="doc-outline" class="js-outline xl:hidden outline-content"></div>
